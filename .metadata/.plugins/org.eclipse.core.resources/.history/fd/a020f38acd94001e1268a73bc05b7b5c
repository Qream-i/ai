package javaBasic3;

public class Teacher {
	
	//멤버 필드, 필드
	String 코드;
	String 이름;
	String 전공;
	int 급여;
	String 구분;
	int 시수;
	
	public Teacher(String 교사코드, String 성함, String 전공명, int 급여, String 구분, int 시수) {
		this.코드 = 교사코드;
		this.이름 = 성함;
		this.전공 = 전공명;
		this.급여 = 급여; // 필드에 있는 급여 변수를 가르켜주고싶다. this 필드의 멤버 변수
		this.구분 = 구분;
		this.시수 = 시수;

	}

	int[] 성적처리() {
		return null;
	}
	
	String[] 레포트검사하기() {
		return null;
	}
	
	void 수업하기() {
		
	}
	
	void 휴강하기() {
		
