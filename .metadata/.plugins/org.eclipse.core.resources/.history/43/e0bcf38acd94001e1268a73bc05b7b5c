package javaBasic3;

// 학교에 대한 것을 만들기
public class Student {
	
	String 학번;
	String 이름;
	String 학과;
	int 점수;
	String 전화번호;
	//생성자 == 클래스 이름과 똑같고 객체화될(힙메모리에 올라갈)때 자동으로 실행된다.
	public Student(String 학번, String 이름, String 학과, int 점수, String 전화번호) {
		// TODO Auto-generated constructor stub
		this.학번 = 학번;
		this.이름 = 이름;
		this.학과 = 학과;
		this.점수 = 점수;
		this.전화번호 = 전화번호;
	}

	//점수계산하기
	int 점수계산() {
		
		return 0;
	}
	
	void 성적출력하기(){
		
	}
	
	void 입력하기(){
		
	}
	
	String 장학금대상(){
		return "홍길동";
	}
	
	boolean 합격여부구하기() {
		return true;
	}

}
