package javaBasic2.ch05.sec03;

public class 확인문제5장 {

	public static void main(String[] args) {
		// 6번
		int[][]array= {
				{95, 86},
				{83, 92, 96},
				{78, 83, 93, 87, 88}
		};
		System.out.println(array.length);
		System.out.println(array[2].length);
		
		//7번
		int[]array1= {1, 5, 3, 8, 2};
		int max=0;
		
		for(int i = 0; i<array1.length; i++) {
			if (array1[i] > max) { 
				max = array1[i];
			}
			System.out.println(max);
		}

	}

}
