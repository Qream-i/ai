package javaBasic.ch03.sec02;

import java.util.Scanner;

public class 산술연산자 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		// 키보드로 글자 입력하는 클래스(class)
		
		//이름은?
		//국어 점수는?
		//영어 점수는?
		//수학 점수는?
		//출력
		//홍길동 100 80 90
		
		System.out.println("이름은?");
		String name = sc.next();
		System.out.println(name);
		
//		int kor = sc.nextInt();
