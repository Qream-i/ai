package javaBasic3.ch06.sec10.exam01;

//247pg
public class Calculator {
	
	static double pi = 3.14159;
	
	static int plus(int x, int y) {
		return x + y ;
	}
	
	static int minus(int x, int y) {
		return x - y;
	}
	
	static int multi(int x, int y) {
		return x * y;
	}
	
	static int div(int x, int y) {
		return x / y;
	}

}
