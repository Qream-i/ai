package javaBasic.ch04.sec01;

public class If문 {

	public static void main(String[] args) {
		// 만약에 조건이 참이면 거짓이면
		// 90~100 "A" 80~90 "B"
		int score = 87;
		if(score>=90) {
			System.out.println("A");
		}
		else(90>=score>=80) {
			System.out.println("B");
		}
		else(80>=score>=70) {
			System.out.println("C");
		}

	}

}
